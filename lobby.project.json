{
    "name": "[project-hades] lobby",
    "tree": {
        "$className": "DataModel",

        "ReplicatedFirst": {
			"$className": "ReplicatedFirst",
			"$ignoreUnknownInstances": true
		},

        "ReplicatedStorage": {
            "$className": "ReplicatedStorage",
            "$ignoreUnknownInstances": true,
            "$path": "Places/Lobby/ReplicatedStorage",

            "Submodules": {
                "$className": "Folder",
                "Core": {
                    "$className": "Folder",
                    "Source": {
                        "$className": "Folder",
                        "$path": "bfg-core/Source/Client"
                    },

                    "Packages": {
                        "$className": "Folder",
                        "$path": "bfg-core/Packages"
                    },
                    
                    "Libraries": {
                        "$className": "Folder",
                        "$path": "bfg-core/Libraries"
                    },

                    "Shared": {
                        "$className": "Folder",
                        "$path": "bfg-core/Source/Shared"
                    }

                },

				"DebugTools": {
					"$path": "bfg-debug/Source/DebugTools"
				}

            }
        },
        
        "ServerScriptService": {
            "$className": "ServerScriptService",
            "$path": "Places/Lobby/ServerScriptService",

            "Submodules": {
                "$className": "Folder",
                "Core": {
                    "$className": "Folder",
                    "Source": {
                        "$className": "Folder",
                        "$path": "bfg-core/Source/Server"
                    }
                }
            }
        },

        "StarterPlayer": {
            "$className": "StarterPlayer",
            
            "$properties": {
                "LoadCharacterAppearance": true,
                "HealthDisplayDistance": 0,
                "NameDisplayDistance": 0,
                "CharacterJumpPower": 0,
                "CharacterUseJumpPower": true,
				"EnableMouseLockOption": false
			},
            
            "StarterCharacterScripts": {
                "$className": "StarterCharacterScripts",
                "$ignoreUnknownInstances": true,
                "$path": "Places/Lobby/StarterPlayer/StarterCharacterScripts"
            },

            "StarterPlayerScripts": {
                "$className": "StarterPlayerScripts",
                "$ignoreUnknownInstances": true,
                "$path": "Places/Lobby/StarterPlayer/StarterPlayerScripts"
            }
        }
    }
}